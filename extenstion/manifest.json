{
  "manifest_version": 3,
  "name": "WarpNest Proxy Switcher",
  "version": "1.0",
  "description": "Auto proxy extension for WarpNest proxies. Authenticate, view, and switch proxies easily.",
  "permissions": [
    "proxy",
    "cookies",
    "storage",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png",
    "256": "icon256.png"
  }
}
