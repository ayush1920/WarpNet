{
  "manifest_version": 3,
  "name": "Proxy Info Overlay",
  "version": "1.0",
  "description": "Displays proxy IP and port on every page",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["config.json"],
      "matches": ["<all_urls>"]
    }
  ]
}
